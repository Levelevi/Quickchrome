{
    "manifest_version": 3,
    "name": "ORGINALtest",
    "version": "1.0",
    "description": "Advanced window and tab management with scheduling capabilities",
    "permissions": [
      "storage",
      "tabs",
      "windows",
      "commands",
      "system.display"
    ],
    "action": {
      "default_popup": "popup.html?mode=popup"
    },
    "background": {
      "service_worker": "background.js"
    },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+1",
          "mac": "Command+Shift+1"
        },
        "description": "Open Quick Launcher"
      }
    }
  }